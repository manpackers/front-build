{
  "name": "@manpacker/noden",
  "version": "1.3.0",
  "description": "Node project generator build project",
  "main": "./lib",
  "bin": {
    "manpacker-noden": "./bin/manpacker-noden"
  },
  "scripts": {
    "test:server": "node ./bin/manpacker-noden server --ic ./test/config/manpacker.ic.js --config ./test/manpacker.config.js",
    "test:build": "node ./bin/manpacker-noden build --ic ./test/config/manpacker.ic.js --config ./test/manpacker.config.js",
    "test:nodemon": "nodemon --watch ./test/app/index.js ./test/app/inde.js 9090"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manpackers/noden.git"
  },
  "keywords": [
    "noden",
    "webpack",
    "manpacker",
    "generator",
    "build",
    "node"
  ],
  "author": {
    "name": "winterfete"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manpackers/noden/issues"
  },
  "homepage": "https://github.com/manpackers/noden#readme",
  "eslintConfig": {
    "extends": [
      "manpacker-typescript"
    ]
  },
  "babel": {
    "presets": [
      "manpacker"
    ]
  },
  "devDependencies": {
    "babel-preset-manpacker": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-manpacker-typescript": "^1.2.0",
    "koa": "^2.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "babel-loader": "^8.0.5",
    "chalk": "^4.1.0",
    "commander": "^4.1.1",
    "cross-spawn": "^7.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "nodemon": "^2.0.7",
    "ora": "^5.1.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.0",
    "webpack": "^4.30.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 5.0.0"
  }
}
