{
  "name": "@manpacker/generator",
  "version": "1.10.0",
  "description": "Frontend generator build project",
  "main": "./lib",
  "bin": {
    "manpacker": "./bin/manpacker"
  },
  "scripts": {
    "test:server": "node ./bin/manpacker server --ic ./__test__/config/manpacker.ic.js --config ./__test__/manpacker.webpack.js",
